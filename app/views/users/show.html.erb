<h1><%= @user.username %></h1>
<h6><%= @user.email %></h6>

<%= link_to "Ask question", new_question_path %>
<div>
	<ul class='search_container'>
		<li>Search by:</li>
		<li><select id="search_by">
			<option value='title'>Title</option>
			<option value='tag'>Tags</option>
		</select> </li>
		<li><input id="description" type="text" /></li>
		<li><input type="button" onclick="searchQuestion()" value="search"/></li>
	</ul>
</div>

<%= javascript_tag do %>
  questions = "<%= @question_data %>";
<% end %>

<script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">
    	console.log(questions);
      google.load("visualization", "1", {packages:["corechart"]});
google.setOnLoadCallback(drawVisualization);
function drawVisualization() {
  var data = [];
  
  
  data[0] = google.visualization.arrayToDataTable([
    ['Month', 'Bolivia', 'Ecuador', 'Madagascar', 'Papua New Guinea', 'Rwanda', 'Average'],
    ['2004/05',  165,      938,         522,             998,           450,      614.6],
    ['2005/06',  135,      1120,        599,             1268,          288,      682],
    ['2006/07',  157,      1167,        587,             807,           397,      623],
    ['2007/08',  139,      1110,        615,             968,           215,      609.4],
    ['2008/09',  136,      691,         629,             1026,          366,      569.6]
  ]);
  var options = {
    title : 'Displying question X-5 / XX',
    vAxis: {title: ""},
    hAxis: {title: "Questions"},
    seriesType: "bars",
    series: {5: {type: "line"}}
  };
  var chart = new google.visualization.ComboChart(document.getElementById('chart_div'));
  chart.draw(data[0], options);
}
    </script>
<div id="chart_div" style="width: 900px; height: 500px;"></div>